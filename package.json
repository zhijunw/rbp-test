    "@vue/eslint-config-typescript": "^4.0.0",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "babel-eslint": "8",
    "cdn-plugins-jousenzhou": "^1.0.1",
    "commitizen": "^4.1.2",
    "commitlint": "^8.3.5",
    "compression-webpack-plugin": "^4.0.0",
    "conventional-changelog-cli": "^2.0.34",
    "eslint": "^6.0.0",
    "eslint-friendly-formatter": "^4.0.1",
